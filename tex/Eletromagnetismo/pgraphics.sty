%grid
\draw[style=help lines] (-1,0) grid +(7,1);
\draw (0,0) grid +(5,1);
%internal p points
\foreach \x in {0.5,1.5,2.5}
  \foreach \y in {0.5}
  {
  \fill [color=red] (canvas cs:x=\x cm,y=\y cm) circle (2pt);
}

\foreach \x in {3.5,4.5}
  \foreach \y in {0.5}
  {
  \fill (canvas cs:x=\x cm,y=\y cm) circle (2pt);
}

\fill [color=red] (-0.1,0.4) rectangle (0.1,0.6);

%External p points

\foreach \y in {0.5}
  \fill [color=red] (-0.5,\y) circle (2pt);
  
\foreach \y in {0.5}
  \draw (5.5,\y) circle (2pt);

\end{tikzpicture} 
\end{center}}

