\ProvidesPackage{equations}

%Arguments are v and f and also the label for the equation
% like \NavierStokesEq{\textbf{v},\textbf{f}, label}
\newcommand{\NavierStokesEq}[3]{\ensuremath{\rho\left( \frac{\partial {#1}}{\partial t}+#1\cdot\nabla #1 \right)=-\nabla p+\mu\nabla^2 #1 + #2}
\label{#3}
}

%Navier Stokes condition
\newcommand{\NScondition}[1]{\ensuremath{\nabla\cdot {#1}=0}}

%Helpful
\newcommand{\SumFour}[1]{\ensuremath{{#1}_{i+1j}^n+{#1}_{i-1j}^n+{#1}_{ij+1}^n+{#1}_{ij-1}^n}}

%Next time step
\newcommand{\NextTimeStep}[2]{\ensuremath{{#1}^{n+1}={#1}^*-\frac{\Delta t}{\rho}p_{#2}}}